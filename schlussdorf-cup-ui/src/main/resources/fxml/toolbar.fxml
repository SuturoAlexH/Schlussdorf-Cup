<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.layout.*?>

<?import javafx.scene.control.Button?>
<FlowPane fx:id="root" alignment="CENTER" hgap="50.0" prefHeight="56.0" prefWidth="600.0" BorderPane.alignment="CENTER" stylesheets="@../css/bootstrap3.css" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/8"
          fx:controller="org.openjfx.ui.toolbar.ToolbarView">
    <children>
        <Button fx:id="addButton" onAction="#addNewResult" text="hinzufügen" />
        <Button fx:id="editButton" onAction="#editResult" text="bearbeiten" />
        <Button fx:id="deleteButton" onAction="#deleteResult" text="löschen" />
        <Button fx:id="imageButton" onAction="#showImage" text="Bild anzeigen" />
        <Button fx:id="certificateButton" onAction="#createCertificates" text="Urkunden erzeugen" />

    </children>
</FlowPane>